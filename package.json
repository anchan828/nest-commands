  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@commitlint/config-lerna-scopes": "13.1.0",
    "@compodoc/compodoc": "1.1.14",
    "@nestjs/common": "8.0.6",
    "@nestjs/core": "8.0.6",
    "@nestjs/platform-express": "8.0.6",
    "@nestjs/testing": "8.0.6",
    "@types/glob": "7.1.4",
    "@types/jest": "26.0.24",
    "@types/node": "16.3.2",
    "@types/prettier": "2.3.2",
    "@types/rimraf": "3.0.1",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "glob": "7.1.7",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "lerna": "4.0.0",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.3.0",
    "supertest": "6.1.4",
    "ts-jest": "27.0.4",
    "ts-node": "10.2.0",
    "typescript": "4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anchan828/nest-commands.git"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "compodoc": "compodoc -c .compodocrc.json",
    "format": "prettier --write '**/*.{js,json,yml,yaml,md}'",
    "lint": "eslint --ignore-pattern packages '**/*.ts' && lerna run --parallel lint",
    "lint:fix": "eslint --ignore-pattern packages --fix '**/*.ts' && lerna run --parallel lint:fix",
    "new": "ts-node scripts/new-package.ts",
    "postinstall": "husky install && npm run bootstrap && npm run build",
    "postnew": "ts-node scripts/generate-vs-launch.ts",
    "publish": "lerna publish --yes patch",
    "publish:major": "lerna publish --yes major",
    "publish:minor": "lerna publish --yes minor",
    "test": "lerna run test && npx @anchan828/merge-coverage",
    "watch": "lerna run --parallel watch"
  },
  "private": true
}
